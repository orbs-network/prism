{
  "name": "prism",
  "version": "0.5.15",
  "description": "Prism, Orbs block explorer",
  "main": "index.js",
  "engines": {
    "node": "10.17.0"
  },
  "scripts": {
    "publish": "node release.js",
    "test": "npm run test-server",
    "type-check": "tsc -p ./src/client/tsconfig.json",
    "test-server": "node env-checker.js && jest --config ./src/server/__tests__/jest.config.js",
    "test-client": "jest --config ./src/client/__tests__/jest.config.js",
    "test-e2e": "node env-checker.js && cross-env DISABLE_ANIMATIONS=true jest --config ./e2e/jest-e2e.config.js",
    "clean": "rimraf dist",
    "postinstall": "npm run build",
    "lint": "tslint -c tslint.json 'src/**/*.ts' 'src/**/*.tsx'",
    "start": "node index.js",
    "start:prod": "NODE_ENV=production node index.js",
    "build-client": "cross-env NODE_ENV=production webpack -p",
    "dev-client": "webpack-dev-server -w",
    "build-server": "tsc -p ./src/server",
    "dev-server": "tsc-watch -p ./src/server --onSuccess \"node index.js\" --noClear",
    "dev": "npm run clean && concurrently --prefix \"[{name}]\" --names \"SERVER,CLIENT\" -c \"bgBlue.bold,bgGreen.bold\" \"npm run dev-server\" \"npm run dev-client\"",
    "build": "npm run clean && concurrently --prefix \"[{name}]\" --names \"SERVER,CLIENT\" -c \"bgBlue.bold,bgGreen.bold\" \"npm run build-server\" \"npm run build-client\""
  },
  "prettier": {
    "trailingComma": "all",
    "tabWidth": 2,
    "semi": true,
    "singleQuote": true,
    "jsxSingleQuote": true,
    "printWidth": 120
  },
  "keywords": [
    "typescript",
    "react",
    "starter-kit",
    "webpack",
    "fullstack",
    "express",
    "express4",
    "node"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/orbs-network/prism.git"
  },
  "license": "MIT",
  "author": "Gil Amran",
  "dependencies": {
    "@types/dotenv": "^8.2.0",
    "@types/find-up": "^4.0.0",
    "@types/gsap": "^1.20.2",
    "@types/mongoose": "^5.5.31",
    "@types/pg": "^7.11.2",
    "@types/react-redux": "^7.1.5",
    "@types/three": "^0.103.2",
    "body-parser": "^1.19.0",
    "cloudinary": "^1.17.0",
    "concurrently": "^5.0.0",
    "cross-env": "^6.0.3",
    "dotenv": "^8.2.0",
    "ejs": "^2.7.2",
    "express": "^4.17.1",
    "find-up": "^4.1.0",
    "gsap": "^3.0.1",
    "mongoose": "^5.7.10",
    "mongoose-long": "^0.2.1",
    "object-hash": "^2.0.0",
    "orbs-blocks-polling-js": "^2.0.1",
    "orbs-client-sdk": "^2.2.2",
    "pg": "^7.12.1",
    "postprocessing": "^6.8.8",
    "query-string": "^6.9.0",
    "react-redux": "^7.1.3",
    "react-spring": "^8.0.27",
    "react-syntax-highlighter": "^11.0.2",
    "react-timeago": "^4.4.0",
    "redux": "^4.0.4",
    "redux-thunk": "^2.3.0",
    "rollbar": "^2.14.4",
    "semver": "^6.3.0",
    "socket.io": "^2.3.0",
    "socket.io-client": "^2.3.0",
    "three": "^0.110.0",
    "tslib": "^1.10.0",
    "winston": "^3.2.1",
    "winston-transport-rollbar": "^2.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.7.2",
    "@babel/plugin-proposal-class-properties": "^7.7.0",
    "@babel/plugin-proposal-decorators": "^7.7.0",
    "@babel/plugin-proposal-numeric-separator": "^7.2.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.6.2",
    "@babel/plugin-transform-runtime": "^7.6.2",
    "@babel/preset-env": "^7.7.1",
    "@babel/preset-react": "^7.7.0",
    "@babel/preset-typescript": "^7.7.2",
    "@material-ui/core": "^4.6.1",
    "@material-ui/icons": "^4.5.1",
    "@types/enzyme": "^3.10.3",
    "@types/expect-puppeteer": "^3.3.3",
    "@types/express": "^4.17.2",
    "@types/jest": "^24.0.23",
    "@types/jest-environment-puppeteer": "^4.3.1",
