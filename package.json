{
  "name": "prism",
  "version": "0.5.7",
  "description": "Prism, Orbs block explorer",
  "main": "index.js",
  "engines": {
    "node": ">=10.6.0"
  },
  "scripts": {
    "publish": "node release.js",
    "test": "npm run test-server",
    "type-check": "tsc -p ./src/client/tsconfig.json",
    "test-server": "node env-checker.js && jest --config ./src/server/__tests__/jest.config.js",
    "test-client": "jest --config ./src/client/__tests__/jest.config.js",
    "test-e2e": "node env-checker.js && cross-env DISABLE_ANIMATIONS=true jest --config ./e2e/jest-e2e.config.js",
    "clean": "rimraf dist",
    "postinstall": "npm run build",
    "lint": "tslint -c tslint.json 'src/**/*.ts' 'src/**/*.tsx'",
    "start": "node index.js",
    "start:prod": "NODE_ENV=production node index.js",
    "build-client": "cross-env NODE_ENV=production webpack -p",
    "dev-client": "webpack-dev-server -w",
    "build-server": "tsc -p ./src/server",
    "dev-server": "tsc-watch -p ./src/server --onSuccess \"node index.js\" --noClear",
    "dev": "npm run clean && concurrently --prefix \"[{name}]\" --names \"SERVER,CLIENT\" -c \"bgBlue.bold,bgGreen.bold\" \"npm run dev-server\" \"npm run dev-client\"",
    "build": "npm run clean && concurrently --prefix \"[{name}]\" --names \"SERVER,CLIENT\" -c \"bgBlue.bold,bgGreen.bold\" \"npm run build-server\" \"npm run build-client\""
  },
  "prettier": {
    "trailingComma": "all",
    "tabWidth": 2,
    "semi": true,
    "singleQuote": true,
    "jsxSingleQuote": true,
    "printWidth": 120
  },
  "keywords": [
    "typescript",
    "react",
    "starter-kit",
    "webpack",
    "fullstack",
    "express",
    "express4",
    "node"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/orbs-network/prism.git"
  },
  "license": "MIT",
  "author": "Gil Amran",
  "dependencies": {
    "@types/dotenv": "^6.1.0",
    "@types/find-up": "^2.1.1",
    "@types/gsap": "^1.20.1",
    "@types/mongoose": "^5.3.20",
    "@types/pg": "^7.4.11",
    "@types/react-redux": "^7.0.1",
    "@types/three": "^0.93.19",
    "body-parser": "^1.18.3",
    "cloudinary": "^1.14.0",
    "concurrently": "^4.1.0",
    "cross-env": "^5.2.0",
    "dotenv": "^6.2.0",
    "ejs": "^2.6.1",
    "express": "^4.16.4",
    "find-up": "^3.0.0",
    "gsap": "^2.0.2",
    "mongoose": "^5.4.17",
    "mongoose-long": "^0.2.1",
    "object-hash": "^1.3.1",
    "orbs-client-sdk": "0.0.6",
    "pg": "^7.8.0",
    "postprocessing": "^5.6.0",
    "query-string": "^6.5.0",
    "react-redux": "^6.0.0",
    "react-spring": "^8.0.18",
    "react-syntax-highlighter": "^10.2.1",
    "react-timeago": "^4.4.0",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0",
    "rollbar": "^2.5.4",
    "semver": "^6.1.1",
    "socket.io": "^2.2.0",
    "socket.io-client": "^2.2.0",
    "three": "^0.101.1",
    "tslib": "^1.9.3",
    "winston": "^3.2.1",
    "winston-transport-rollbar": "^2.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.2.3",
    "@babel/plugin-proposal-numeric-separator": "^7.2.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/preset-env": "^7.2.3",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.1.0",
    "@material-ui/core": "^3.9.2",
    "@material-ui/icons": "^3.0.2",
    "@types/enzyme": "^3.1.15",
    "@types/expect-puppeteer": "^3.3.0",
    "@types/express": "^4.16.0",
    "@types/jest": "^23.3.14",
    "@types/jest-environment-puppeteer": "^2.2.1",
    "@types/node": "^10.12.18",
    "@types/puppeteer": "^1.11.1",
    "@types/react": "^16.7.18",
    "@types/semver": "^6.0.0",
    "@types/socket.io": "^2.1.2",
    "@types/socket.io-client": "^1.4.32",
    "axios": "^0.18.0",
    "babel-loader": "^8.0.5",
