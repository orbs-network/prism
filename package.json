    "request": "^2.88.0",
    "rimraf": "^2.6.3",
    "source-map-loader": "^0.2.4",
    "style-loader": "^0.23.1",
    "ts-jest": "^23.10.5",
    "tsc-watch": "^1.1.35",
    "tslint": "^5.12.1",
    "typescript": "^3.5.2",
    "url-loader": "^1.1.2",
    "webpack": "^4.28.4",
    "webpack-bundle-analyzer": "^3.0.3",
    "webpack-cli": "^3.2.1",
    "webpack-dev-server": "^3.1.14",
    "webpack-manifest-plugin": "^2.0.4"
  }
}
